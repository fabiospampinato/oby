{
  "name": "oby",
  "repository": "github:vobyjs/oby",
  "description": "A tiny Observable implementation, the brilliant primitive you need to build a powerful reactive system.",
  "version": "12.7.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "all": "npm run compile && npm run compile:mangle && npm run test && npm run benchmark && npm run cleanup && npm run creation && npm run memory && npm run read && npm run store && npm run store:creation && npm run updates",
    "benchmark": "tsex benchmark",
    "benchmark:watch": "tsex benchmark --watch",
    "clean": "tsex clean",
    "cleanup": "node tasks/cleanup.js",
    "compile": "tsex compile",
    "compile:mangle": "mangler --folder dist --ext js --props 'fnWithIndex,key,insert,node,getters,setters,properties,selecteds,source,listen,listened,listeners,listenersRegular,listenersRoots,registerListener,unregisterListener,fn,bool,read,wrap,after,cache,count,emit,stale,source,index,value,valueReadonly,write,signal,before,errors,parent,parents,pausable,target,result,update,toggle,frozen,current,dispose,unstale,inactive,effects,cleanups,contexts,roots,readable,writable,nextCount,observers,prevCount,suspenses,suspended,observables,postdispose,statusCount,statusFresh,registerSelf,registerError,registerEffect,statusExecution,registerCleanup,unregisterEffect,registerSuspense,registerObserver,unregisterSuspense,registerObservable,unregisterObserver,registerRoot,unregisterRoot,observablesLeftover'",
    "compile:watch": "tsex compile --watch",
    "creation": "node tasks/creation.js",
    "memory": "node tasks/memory.js",
    "read": "node tasks/read.js",
    "store": "node tasks/store.js",
    "store:creation": "node tasks/store.creation.js",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "updates": "node tasks/updates.js",
    "prepublishOnly": "npm run clean && npm run compile && npm run compile:mangle && npm run test"
  },
  "keywords": [
    "observable",
    "signal",
    "reactive",
    "reactivity",
    "tiny",
    "small",
    "fast"
  ],
  "devDependencies": {
    "benchloop": "^2.1.0",
    "fava": "^0.2.0",
    "ispy": "^0.1.2",
    "js-simple-mangler": "^2.0.0",
    "tape": "^5.6.3",
    "tsex": "^2.2.2",
    "typescript": "^4.9.5"
  }
}
